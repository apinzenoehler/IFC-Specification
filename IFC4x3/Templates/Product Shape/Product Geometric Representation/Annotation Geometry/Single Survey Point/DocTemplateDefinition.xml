<?xml version="1.0" encoding="utf-8"?>
<DocTemplateDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" Name="Single Survey Point" UniqueId="f93a9736-c813-58e1-b005-98de267abcee" Status="Sample" Type="IfcAnnotation">
	<Localization>
		<DocLocalization Locale="en">
